<h1>ログインページ</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_with url: login_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "ログイン"%>
<% end %>  